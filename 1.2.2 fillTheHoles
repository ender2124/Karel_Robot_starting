/*
F1 = moveForward();
F2 = turnLeft();
F3 = turnAround();
F4 = turnRight();
F5 = pickBeeper();
F6 = dropBeeper();
*/

/* FELIPE 23.11.2020*/
/* 1.2.2 fillTheHoles */

void  fillTheHoles ()
{
    
    leaveBeeper ();
}

void leaveBeeper ()
{
    repeat (4)
    {
        moveForward();
        turnRight();
        moveForward();
        dropBeeper();
        turnAround();
        moveForward();
        turnRight();
        moveForward();
    }
}