/* FELIPE 22.11.2020*/
/* 1.2.1 climbTheStairs */

void  climbTheStairs ()
{
    moveForward();
    climbingStairs ();
}

void climbingStairs ()
{
    repeat (6)
    {
        turnLeft();
        moveForward();
        turnRight();
        moveForward();
    }
}